   "source": [
    "n = int(input())\n",
    "l_list = list(map(int, input().split()))\n",
    "\n",
    "max_rope = max(l_list)\n",
    "l_list.remove(max_rope)\n",
    "if max_rope > sum(l_list):\n",
    "    n = max_rope - sum(l_list)\n",
    "else:\n",
    "    n = max_rope + sum(l_list)\n",
    "    \n",
    "print(n)"
   ]
  }
